{
  "name": "xefi/faker-php-locales-en-pk",
  "type": "library",
  "description": "Faker extension to include en_PK locale",
  "keywords": [
    "faker",
    "data",
    "en",
    "pk"
  ],
  "license": "MIT",
  "authors": [
    {
      "name": "Martin Soenen"
    }
  ],
  "require": {
    "php": "^8.3",
    "psr/container": "^2.0",
    "xefi/faker-php": "^1"
  },
  "require-dev": {
    "phpunit/phpunit": "^11"
  },
  "autoload": {
    "psr-4": {
      "Xefi\\Faker\\EnPk\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Xefi\\Faker\\EnPk\\Tests\\": "tests/"
    }
  },
  "config": {
    "sort-packages": true
  },
  "extra": {
    "faker": {
      "providers": [
        "Xefi\\Faker\\EnPk\\FakerEnPkServiceProvider"
      ]
    }
  }
}
